{
  "name": "moleculer-antivirus",
  "version": "1.2.0",
  "description": "An antivirus service for the moleculer framework",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/designtesbrot/moleculer-antivirus"
  },
  "author": "caspar.bauer0@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev": "yarn install && nodemon examples/index.js",
    "demo": "yarn install && node examples/index.js",
    "ci": "yarn install && jest --watch",
    "test": "yarn install && jest --coverage --detectOpenHandles",
    "lint": "yarn install && eslint --ext=.js src",
    "deps": "yarn install && yarn-check -u",
    "postdeps": "yarn test",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "docgen": "node ./node_modules/moleculer-docgen src/service.js"
  },
  "keywords": [
    "clamav",
    "antivirus",
    "microservices",
    "microservice",
    "moleculer"
  ],
  "devDependencies": {
    "coveralls": "3.0.2",
    "eslint": "5.6.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "moleculer": "^0.13.3",
    "moleculer-docgen": "^0.2.1",
    "moleculer-repl": "^0.5.2",
    "moleculer-web": "^0.8.2",
    "multiparty": "^4.2.1",
    "nats": "^1.0.1",
    "node-res": "^5.0.1",
    "nodemon": "1.18.4",
    "yarn-check": "0.0.3"
  },
  "peerDependencies": {
    "moleculer": ">= 0.13.0"
  },
  "dependencies": {
    "clamav.js": "^0.12.0",
    "file-type": "^10.1.0",
    "node-fetch": "^3.2.10",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.10.0"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ]
  }
}
